{
  "name": "testing-mandatory-one",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "startbuild": "tsc -b & node build/app.js",
    "startdev": "nodemon --exec yarn startbuild",
    "start": "node build/app.js",
    "tdd": "nodemon --exec jest",
    "prisma": "prisma init --datasource-provider mysql && prisma db pull  && pal schema camel-case && npx prisma generate",
    "prisma-generate": "npx prisma generate --schema ./prisma/mysql/schema.prisma && npx prisma generate --schema ./prisma/mongodb/schema.prisma",
    "dev": "yarn install && yarn startdev",
    "devthor": "npx nodemon ./src/app.ts",
    "finnhub-swagger": "npx swagger-typescript-api -p https://finnhub.io/static/swagger.json -o ./src/swagger/finnhub.io",
    "postinstall": "tsc && prisma-generate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@types/bson": "^4.2.0",
    "@types/mongodb": "^4.0.7",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "neo4j-driver": "^4.4.5",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.7",
    "@paljs/cli": "^4.0.18",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "add": "^2.0.6",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "prisma": "^3.12.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "yarn": "^1.22.18"
  },
  "nodemonConfig": {
    "ignore": [
      "build/"
    ],
    "delay": 4000,
    "ext": "js, ts, mjs, json, html, css"
  }
